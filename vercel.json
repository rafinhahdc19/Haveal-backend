{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.js"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE": "true"
    },
  "scripts": {
    "build": "npm run prisma:generate && <outro comando de construção, se necessário>"
  }
    "commands": [
      "npm install -g prisma",
      "prisma generate"
    ]
  }
}
